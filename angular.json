{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vms-admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/vms-admin",
      "sourceRoot": "projects/vms-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "projects/vms-admin/extra-webpack.config.js",
              "mergeStrategies": {
                "module.rules": "prepend"
              },
              "replaceDuplicatePlugins": true
            },
            "outputPath": "dist/vms-admin",
            "index": "projects/vms-admin/src/index.html",
            "main": "projects/vms-admin/src/main.ts",
            "polyfills": "projects/vms-admin/src/polyfills.ts",
            "tsConfig": "projects/vms-admin/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/vms-admin/src/favicon.ico",
              "projects/vms-admin/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/vms-admin/src/assets/css/font-feather.css",
              "projects/vms-admin/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.js",
              "./node_modules/zingchart/zingchart.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/vms-admin/src/environments/environment.ts",
                  "with": "projects/vms-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "customWebpackConfig": {
              "path": "projects/vms-admin/extra-webpack.config.js",
              "mergeStrategies": {
                "module.rules": "prepend"
              },
              "replaceDuplicatePlugins": true
            },
            "browserTarget": "vms-admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "vms-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vms-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/vms-admin/src/test.ts",
            "polyfills": "projects/vms-admin/src/polyfills.ts",
            "tsConfig": "projects/vms-admin/tsconfig.spec.json",
            "karmaConfig": "projects/vms-admin/karma.conf.js",
            "assets": [
              "projects/vms-admin/src/favicon.ico",
              "projects/vms-admin/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/vms-admin/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.js",
              "./node_modules/bootstrap/dist/js/bootstrap.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/vms-admin/tsconfig.app.json",
              "projects/vms-admin/tsconfig.spec.json",
              "projects/vms-admin/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/vms-admin/e2e/protractor.conf.js",
            "devServerTarget": "vms-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "vms-admin:serve:production"
            }
          }
        }
      }
    },
    "widgets": {
      "projectType": "library",
      "root": "projects/widgets",
      "sourceRoot": "projects/widgets/src",
      "prefix": "lib",
      "architect": {
        "build": {
